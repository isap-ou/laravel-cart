{
  "name": "isapp/laravel-cart",
  "description": "Is high customazible package for adding shopping cart functionality to Laravel applications",
  "type": "library",
  "keywords": [
    "framework",
    "laravel",
    "laravel-package",
    "php",
    "cart",
    "e-commerce"
  ],
  "require": {
    "php": "^8.2",
    "ext-bcmath": "*",
    "illuminate/support": "^10|^11|^12"
  },
  "support": {
    "email": "contact@isap.me"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Isapp\\LaravelCart\\": "src/",
      "Isapp\\LaravelCart\\Database\\Factories\\": "database/factories/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Isapp\\LaravelCart\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "ISAP OÃœ",
      "email": "contact@isap.me",
      "homepage": "https://isap.me"
    },
    {
      "name": "Andrii Trush",
      "email": "andrii.trush@isapp.be",
      "homepage": "https://isapp.be"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Isapp\\LaravelCart\\CartServiceProvider"
      ]
    }
  },
  "require-dev": {
    "laravel/pint": "^1.24"
  }
}
